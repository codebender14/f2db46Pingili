extends layout

block content
  h1= title
  p Welcome to #{title}
  p This is Assignment 10 of 44-563-Web-Apps-F22
  button(type="button", class="btn btn-success" onclick = "doBump()") Press Here
  p
  div#bump Bump the count

  script.  
    var count = 0
    var bump = 1
    function doBump() {
      count = count + bump 
      bump = bump + 1
      document.getElementById("bump").innerHTML = "Count "+ count +" Bump "+bump;
    }